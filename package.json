{
  "name": "functional-programming-unit-testing",
  "version": "1.0.0",
  "description": "Code I wrote to learn unit testing by learning functional programming first",
  "scripts": {
    "server": "./node_modules/httpster/bin/httpster -p 3000 -d ./build/",
    "copy-jquery": "ncp node_modules/jquery/dist/jquery.js build/static/test/jquery.js",
    "copy-blanket": "ncp node_modules/blanket/dist/qunit/blanket.min.js build/blanket.min.js",
    "copy-qunit-js": "ncp node_modules/qunitjs/qunit/qunit.js build/test/qunit.js",
    "copy-qunit-css": "ncp node_modules/qunitjs/qunit/qunit.css build/test/qunit.css",
    "copy-all-qunit": "npm run copy-qunit-js && npm run copy-qunit-css",
    "start": "npm run server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kaidez/functional-programming-unit-testing.git"
  },
  "keywords": [
    "qunit",
    "unit testing",
    "functional programming"
  ],
  "author": "kaidez",
  "license": "ISC",
  "homepage": "n/a",
  "devDependencies": {
    "blanket": "^1.2.3",
    "httpster": "^1.0.3",
    "ncp": "^2.0.0",
    "qunitjs": "^2.2.0"
  },
  "dependencies": {
    "jquery": "^3.2.0"
  }
}
